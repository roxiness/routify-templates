{
  "name": "routify-templates",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "canvasit --watch -output output",
    "help": "canvasit help",
    "generate": "canvasit -output output",
    "test:output:dev": "cd output && npm run test:dev",
    "test:output:build": "cd output && npm run test:build",
    "build": "node generate"
  },
  "author": "",
  "license": "ISC",
  "canvasit": {
    "basepath": "fragments",
    "@exec": "pnpm install && pnpm dev",
    "ignore": [
      "node_modules",
      "dist",
      ".routify"
    ],
    "include": [
      "base"
    ]
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "playwright": "^1.7.1",
    "prettier-plugin-svelte": "^2.1.0",
    "start-server-and-test": "^1.11.7",
    "svelte": "^3.31.2"
  },
  "ava": {
    "ignoredByWatcher": [
      "**/.routify",
      "**/dist"
    ]
  },
  "dependencies": {
    "canvasit": "^0.1.1"
  }
}
