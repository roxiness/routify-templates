{
  "name": "routify-templates",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "canvasit --watch -output output",
    "generate": "canvasit -output output",
    "test": "ava",
    "test:builds": "ava templates/**/*.test.js",
    "test:output": "ava output/**/*.test.js",
    "build": "node generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "canvasit": "^0.0.5"
  },
  "canvasit": {
    "basepath": "fragments",
    "@exec": "pnpm install && pnpm dev",
    "ignore": [
      "node_modules",
      "dist",
      ".routify"
    ],
    "include": [
      "base"
    ]
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "playwright": "^1.7.1"
  },
  "ava": {
    "ignoredByWatcher": [
      "**/.routify",
      "**/dist"
    ]
  }
}